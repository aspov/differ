<?xml version="1.0" encoding="UTF-8"?>
<phpunit colors="true">
  <filter>
    <whitelist processUncoveredFilesFromWhitelist="true">
      <directory suffix=".php">./src</directory>
    </whitelist>
  </filter>
  <logging>
    <log type="coverage-clover" target="./clover.xml"/>
  </logging>
</phpunit>
